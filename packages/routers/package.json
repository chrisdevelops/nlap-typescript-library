{
  "name": "@nlap/routers",
  "version": "0.3.0",
  "description": "Routing implementations for NLAP - Keyword, Embedding, and Hybrid routers for action selection",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "nlap",
    "router",
    "embedding",
    "keyword",
    "nlp",
    "tf-idf",
    "semantic-search",
    "action-routing",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/nlap-typescript-library.git",
    "directory": "packages/routers"
  },
  "homepage": "https://github.com/yourusername/nlap-typescript-library#readme",
  "bugs": {
    "url": "https://github.com/yourusername/nlap-typescript-library/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.0",
    "natural": "^7.0.7",
    "stopword": "^3.0.1"
  },
  "devDependencies": {
    "@types/natural": "^5.1.5",
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^1.6.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
