{
  "name": "@nlap/core",
  "version": "0.3.0",
  "description": "Core engine for Natural Language Action Parser - Transform natural language into validated, executable action plans",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "nlp",
    "natural-language",
    "action-parser",
    "llm",
    "ai",
    "typescript",
    "automation",
    "cli"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/nlap-typescript-library.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/yourusername/nlap-typescript-library#readme",
  "bugs": {
    "url": "https://github.com/yourusername/nlap-typescript-library/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@nlap/providers": "*",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.4"
  },
  "peerDependencies": {
    "@nlap/providers": "*",
    "@nlap/routers": "*"
  },
  "peerDependenciesMeta": {
    "@nlap/providers": {
      "optional": true
    },
    "@nlap/routers": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
